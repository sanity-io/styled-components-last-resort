{
  "name": "css-in-js-root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "format": "prettier --write .",
    "postinstall": "pnpm -r generateErrors",
    "lint": "oxlint --type-aware",
    "test": "pnpm -r test"
  },
  "prettier": "@sanity/prettier-config",
  "dependencies": {
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.6",
    "@sanity/prettier-config": "^2.0.1",
    "@types/node": "^24.3.0",
    "oxlint": "^1.13.0",
    "oxlint-tsgolint": "^0.1.1",
    "prettier": "^3.6.2"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "react",
        "react-dom",
        "react-is"
      ]
    },
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom"
    }
  }
}
